image/png;                      feh %s
image/gif;                      feh %s

# PDF
application/pdf;                /home/phinze/.mutt/scripts/view_attachment.sh %s pdf /usr/bin/apvlv

# Octet Stream?
application/octet-stream;       /home/phinze/.mutt/scripts/view_attachment.sh %s

